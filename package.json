{
  "name": "gitbook-plugin-resources",
  "version": "1.3.1",
  "description": "a gitbook plugin optional resources loading of gitbook",
  "engines": {
    "gitbook": ">=3.0.0-pre.0"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleen42/gitbook-resources.git"
  },
  "keywords": [
    "gitbook-plugin",
    "gitbook",
    "resources"
  ],
  "author": "aleen42 <aleen42@vip.qq.com> (http://aleen42.github.io/pc.html)",
  "contributors": [
    "aleen42 <aleen42@vip.qq.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aleen42/gitbook-resources/issues"
  },
  "homepage": "https://github.com/aleen42/gitbook-resources#readme",
  "gitbook": {
    "properties": {
      "type": {
        "type": "string",
        "default": "local",
        "title": "point out where to get resources (local, github)"
      },
      "repo": {
        "type": "string",
        "default": "aleen42/PersonalWiki",
        "description": "user name and the repo name if you choose github"
      },
      "branch": {
        "type": "string",
        "default": "master",
        "description": "which branch you may use if you choose github"
      }
    }
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.0"
  }
}
